/*Slide show*/
.jssorb05{
    position: absolute;
    top:400px;
}

.jssorb05 div, .jssorb05 div:hover, .jssorb05 .av {
    background: url(../img/slideshow/bg01.png) no-repeat;
    overflow: hidden;
    cursor: pointer;
}

.jssorb05 div {
    background-position: -47px -3px;
}

.jssorb05 div:hover, .jssorb05 .av:hover {
    background-position: -18px -3px;
}

.jssorb05 .av {
    background-position: -18px -3px;
}

.slideshow .container{
    padding: 0;
}

@media (min-width: 1355px){
    .slideshow .container {
        width: 1349px !important;
    }
}

.slideshow .container {
    width: 100% !important;
}

#slider1_container{
    position: relative;
    width: 100%;
    height: 530px;
    overflow: hidden; 
    margin: 0 auto;
    margin-top: -20px;
    text-align: center;
}

.slideshow_cont{
    cursor: pointer;
    width: 100%;
    height: 400px;
    margin: 60px 0;
    overflow: hidden;
    font-family: SourceSansPro_Regular;
}

#slider1_container .slideshow_cont .slide_cont .slide_show_cont img{
    margin: 30px 0 20px 0;
}

#slider1_container .slideshow_cont .slide_cont .slide_show_cont .cont_name_show{
   font-size: 23pt;
   color: #1aa5e7;
}

#slider1_container .slideshow_cont .slide_cont .slide_show_cont .cont_cont_show{
    font-size: 13pt;
    color: #6e6e6e;
}

