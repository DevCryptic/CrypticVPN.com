body{
  margin: 0 auto;
  padding: 0;
}

#wap_head{
  position:relative;  
}

.moblogo{
  width:100%; 
}

ul{
  list-style: none;
  margin: 0;
  padding: 0;
}

a{
  text-decoration: none;
}

a:hover{
  text-decoration: none !important;
}

/*Header*/
.one {
    position: relative;
    z-index: 9;
    min-height: auto;
    margin-top: -220px;
    padding-top: 150px;
    padding-bottom: 150px;
    background-position: center 0px;
    background-repeat: no-repeat;
    background-color: transparent;
    color: #fff;
    height: 900px;
    font-family: SourceSansPro_Regular;
}

.one .left{
    float: left;
    margin-top: 170px;
    margin-left: 100px;
}

.one .container .right{
    float: right;
    margin-top: 170px;
    margin-right: 108px;
}

.one a:link {
    display: block;
    color: white;
    float: left;
    margin-left: 10px;
    margin-right: 10px;
}

.one .container .right a{
    color: #fff;
    font-size: 14pt;
}

.one .container .right a:hover{
    color: #ddd;
}

.one .container #head_bottom{
    margin-top:360px;
    margin-right: auto;
    margin-left: auto;
    width: 100%;
    text-align: center;
}

.one .container #head_bottom #p_head_titie{
    font-size: 30pt;
}

.one .container #head_bottom #p_head_content{
    font-size: 14pt;
}

#bt_learnMore,#bt_viewPricing{
    color: #fff;
    font-family: SourceSansPro_Semibold;
    padding: 4px 15px;
    margin: 0 10px;
}

#bt_learnMore{
    border: 1px solid #fff;
    background:url(../img/bg_input.png) no-repeat;
}

#bt_viewPricing{
    background-color: #1aa5e7;
    opacity: 1;
    border: 1px solid #1aa5e7;
}

#menu_head_mb{
    display: none;
}

#icon_menu{
    background-color: #F5F5F5;
    height: 34px;
    cursor: pointer;
}

#menu_head_mb #ul_head_menu li{
    padding: 5px 0 5px 10px;
    background-color: #F5F5F5;
    border-top: 1px solid #fff;
    border-bottom: 1px solid #fff;
    cursor: pointer;
}

#menu_head_mb #ul_head_menu .li_head_memu a{
    color: #000;
    font-family: SourceSansPro_Bold;
    font-size: 15pt;
}

#menu_head_mb #ul_head_menu .li_head_memu:hover a{
    font-weight: bold;
}

@media only screen  and (min-width: 220px) and (max-width: 459px){
    .one .container .left{
        margin-left: 50px !important;
    }
}

@media only screen  and (min-width: 460px) and (max-width: 599px){
    .one .container .left{
        margin-left: 120px !important;
    }
}

@media only screen  and (min-width: 600px) and(max-width: 699px){
    .one .container .left{
        margin-left: 180px !important;
    }
}

@media only screen  and (min-width: 700px) and (max-width: 899px){
    .one .container .left{
        margin-left: 260px !important;
    }
}


@media only screen  and (max-width: 1000px){
    .one{
        height: 600px !important;
    }

    .head_top_menu{
        display: none;
    }

    #head_bottom{
        display: none;
    }

    #menu_head_mb{
        display: block;
    }

    .head_top_logo{
        width: 100% !important;
    }

    .one .container .right{
        display: none;
    }

    #ul_head_menu{
        display: none;
    }
}

@media only screen  and (min-width: 1000px) and (max-width: 1200px){
    .one .left{
        margin-left: 30px !important;
    }

    .one .right{
        margin-right: 0px !important
    }
}

/*swapper*/
#wap_content #wap_cont .div_content{
  margin: 0 auto;
  width: 80%;
}

#wap_content #wap_cont .div_content .icon{
  background-color: #1acaf6;
  width: 60px;
  height: 60px;
  border-radius: 29px;
  position: relative;
  margin: 10px;
}


.friendui{
  background-color: #6B7377;
}

#wap_friendUi,#wap_secure,#wap_multiple_countries{
  background-color: #6B7377;
  height: 400px;
}

.div_friendui_content,.div_secure_content,.div_countries_content{
  width: 40%;
  color: #fff;
}

.div_friendui_content #p_content_title{
  color: white;
}

.div_friendui_content,.div_countries_content{
  margin: 110px 0 0 100px;
  float: left;
}

#p_content_title{
  font-family: SourceSansPro_Regular;
  font-size: 20pt;
}

.p_content_cont{
  font-family: SourceSansPro_ExtraLight;
  font-size: 13pt;
}

.div_img_friend,.div_img_secure,.div_img_multiple{
  width: 38%;
}

.img_friend,.img_secure,.img_multiple{
  width: 100%;

}

.div_img_friend,.div_img_multiple{
  margin: 70px 120px 0 0;
  float: right;
}

.secure{
  background-color: #f58e05;
}

#wap_secure{
  clear: both;
  background-color: #f58e05;
}

.div_secure_content{
  float: right;
  margin: 100px 100px 0 0;
}

.div_img_secure{
  float: left;
  margin: 70px 0 0 100px;
}

.multiple{
  background-color: #1c9b70;
}

#wap_multiple_countries{
  clear: both;
  background-color: #1c9b70;
}

@media only screen  and (max-width: 1000px){
  #wap_friendUi,#wap_secure,#wap_multiple_countries{
    text-align: center !important;
  }

  #wap_multiple_countries{
    height: 385px !important;
  }

  #wap_friendUi{
    height: 475px !important;
  }

  #wap_secure{
    height: 432px !important;
  }

  .div_img_friend,.div_img_multiple{
    margin: 0 auto !important;
    width: 100% !important;
  }

  .div_img_secure{
    margin: 0 auto !important;
    width: 100% !important;
  }

  .div_friendui_content,.div_secure_content,.div_countries_content{
    width: 100% !important;
    margin: 0 !important;
  }

  .img_friend,.img_secure,.img_multiple{
    width: 70% !important;
    margin: 0 auto !important;
  }

  #p_content_title{
    font-size: 15pt;
  }

  .p_content_cont{
    font-size: 12pt;
  }
}

@media only screen and (min-width: 1000px) and (max-width: 1200px){
  #wap_friendUi,#wap_secure,#wap_multiple_countries{
    height: 420px !important;
  }

  .div_img_friend,.div_img_multiple{
    margin-right: 70px !important;
    margin-top:100px !important;
    width: 45%;
  }

  .div_img_secure{
    margin-left: 70px !important;
    margin-top:80px !important;
    width: 45%;
  }

  .img_friend,.img_secure,.img_multiple{
    margin: 0 auto !important;
  }

  .div_friendui_content,.div_countries_content{
    margin-left: 50px !important;
  }

  .div_secure_content{
    margin-right: 40px !important;  
  }
}

/*content*/
.section_content .container{
