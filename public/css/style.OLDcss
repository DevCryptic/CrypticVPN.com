  }

  .section_content .container div.col-lg-10,.section_content .container .row >div{
    padding: 0 !important;
  }
}

@media only screen and (max-width: 400px){
  .section_content .container .row .icon{
    width: 70px;
    height: 70px;
  }
  .section_content .container div.col-lg-10,.section_content .container .row >div{
    padding: 0 !important;
  }
}

@media only screen and (max-width: 1000px){
  .section_content .container .row .col-md-6 {
      width: 100%;
  }
}

